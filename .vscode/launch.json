{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Sphinx HTML",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx.cmd.build",
            "args": "-b html docs/source docs/_build",
            "env": {"PYTHONPATH": "${workspaceFolder}/src"},
        },
        {
            "name": "Sphinx Word",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx.cmd.build",
            "args": "--b word -D master_doc=wordindex docs/source docs/_buildword",
            "env": {"PYTHONPATH": "${workspaceFolder}/src"},
        },
        {
            "name": "Sphinx Latex",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx.cmd.build",
            "args": "--b latex -D master_doc=wordindex docs/source docs/_buildlatex",
            "env": {"PYTHONPATH": "${workspaceFolder}/src"},
        },
        {
            "name": "Python Debugger: unittest",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/skconfig/tests/test_config.py",
            "console": "integratedTerminal",
            "args": "",
            "env": {"PYTHONPATH": "${workspaceFolder}/src"},
            "justMyCode": false
        },

    ]
}